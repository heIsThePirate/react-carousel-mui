{
  "name": "react-mui-carousel",
  "version": "1.0.0",
  "license": "MIT",
  "author": "heIsThePirate",
  "description": "A dynamic and customisable carousel component built using React and MUI",
  "homepage": "https://heIsThePirate.github.io/react-mui-carousel",
  "repository": {
    "type": "git",
    "url": "https://github.com/heIsThePirate/react-mui-carousel"
  },
  "keywords": [
    "react",
    "mui",
    "material",
    "material ui",
    "carousel",
    "slider"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "react",
      "env",
      "stage-2"
    ]
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "test:watch": "jest --config jestconfig.json --watch",
    "test:coverage": "jest --config jestconfig.json --coverage",
    "start": "react-scripts start",
    "build": "npx rimraf dist && npm run build:cjs",
    "build:cjs": "tsc --module commonjs --outDir dist",
    "format": "prettier --write \"src/**/*.tsx\" \"src/**/*.ts\" \"src/**/*.jsx\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.3.1",
    "@types/estree": "0.0.50",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "gh-pages": "^2.2.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "react-scripts": "^5.0.0",
    "ts-jest": "^27.1.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "@mui/system": "^5.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
